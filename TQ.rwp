    <LUACode>function onValueChanged()
	if getValue()==3 then
		if DB.getValue(window.getDatabaseNode(), "PuntosDeVida.Debilitado", 0)==0 then
			DB.setValue(window.getDatabaseNode(), "PuntosDeVida.Debilitado", "number", 1);
			Herramientas.Mensaje(DB.getValue(window.getDatabaseNode(), "name", "") .. " queda debilitado por llegar a Estorbo 3", "msgfont", "action_sobrecarga", false, DB.getValue(window.getDatabaseNode(), "name", ""));
		elseif DB.getValue(window.getDatabaseNode(), "PuntosDeVida.Debilitado", 0)==1 and DB.getValue(window.getDatabaseNode(), "PuntosDeVida.Incapacitado", 0)==0 then
			DB.setValue(window.getDatabaseNode(), "PuntosDeVida.Incapacitado", "number", 1);
			Herramientas.Mensaje(DB.getValue(window.getDatabaseNode(), "name", "") .. " queda incapacitado por llegar a Estorbo 3 estando debilitado", "msgfont", "action_sobrecarga", false, DB.getValue(window.getDatabaseNode(), "name", ""));
		end
	elseif getValue()&gt;=4 and DB.getValue(window.getDatabaseNode(), "PuntosDeVida.Incapacitado", 0)==0 then
		DB.setValue(window.getDatabaseNode(), "PuntosDeVida.Incapacitado", "number", 1);
		Herramientas.Mensaje(DB.getValue(window.getDatabaseNode(), "name", "") .. " queda incapacitado por llegar a Estorbo 4", "msgfont", "action_sobrecarga", false, DB.getValue(window.getDatabaseNode(), "name", ""));
	end
end

function onInit()

	if window.Fuerza.getValue()==0 then
		setValue(0);
	else
		setValue(math.floor(window.encumbranceload.getValue()/window.Fuerza.getValue()));
	end
end
</LUACode>
