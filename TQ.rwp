    <LUACode>function onValueChanged()
	if window.Fuerza.getValue()==0 then
		window.Estorbo.setValue(math.floor(window.PesoTotal.getValue()));
	else
		window.Estorbo.setValue(math.floor(window.PesoTotal.getValue()/window.Fuerza.getValue()));
	end
end

