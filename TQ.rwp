    <LUACode>function onInit()
	setValue(nil, nil);
end

function onDrop(x, y, dragdata)
	local sClass,sRecord = dragdata.getShortcutData();
	if sClass=="VentanaEtniaSidebar" then
		local _, sRecordAct=getValue();
		--ParseoHabilidades.QuitarHabilidades(sRecordAct, "etnia");
		ParseoHabilidades.ModificarHabilidades(sRecordAct, "etnia", false);
		setValue(dragdata.getShortcutData());
		window.NombreEtnia.Actualizar(sRecord);
		--ParseoHabilidades.AniadirHabilidades(sRecord, "etnia");
		ParseoHabilidades.ModificarHabilidades(sRecord, "etnia", true);
		local sPathNombre="";
		
		if string.find(sRecord, "@") then
			sPathName, _=string.gsub(sRecord, "@", ".IdiomaNativo@");
		else
			sPathName=sRecord..".IdiomaNativo";
		end
		ParseoHabilidades.IdiomaNativo = DB.getValue(sPathName, "");
		
		if string.find(sRecord, "@") then
			sPathName, _=string.gsub(sRecord, "@", ".TamanioMaximo@");
		else
			sPathName=sRecord..".TamanioMaximo";
		end
		window.MaxTamanioEtnia.setValue(DB.getValue(sPathName, ""));

		if string.find(sRecord, "@") then
			sPathName, _=string.gsub(sRecord, "@", ".PP@");
		else
			sPathName=sRecord..".PP";
		end
		window.PPEtnia.setValue(DB.getValue(sPathName, ""));
		
		if string.find(sRecord, "@") then
			sPathName, _=string.gsub(sRecord, "@", ".Profesiones@");
		else
			sPathName=sRecord..".Profesiones";
		end		
		ParseoProfesiones.ObtenerProfesionesPermitidas(DB.getValue(sPathName, ""));
	end;
end
