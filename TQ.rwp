	local nAncho,_=getSize();
	window.Labelmarca3.setVisible(nAncho&gt;=717);
	window.LabelExp3.setVisible(nAncho&gt;=717);
	window.LabelHabilidad3.setVisible(nAncho&gt;=717);
	window.LabelTotal3.setVisible(nAncho&gt;=717);

	window.Labelmarca4.setVisible(nAncho&gt;=956);
	window.LabelExp4.setVisible(nAncho&gt;=956);
	window.LabelHabilidad4.setVisible(nAncho&gt;=956);
	window.LabelTotal4.setVisible(nAncho&gt;=956);
end

function onInit()
	window.onSizeChanged = handleSizeChanged;
	if window.getDatabaseNode().getChild("Habilidades")==nil or
	   window.getDatabaseNode().getChild("Habilidades").getChild("ListaHabilidades")==nil or
	   window.getDatabaseNode().getChild("Habilidades").getChild("ListaHabilidades").getChildCount()==0 then
		Habilidades.CrearHabilidades(window);
	end;
end


