    <LUACode>local nContDados=1;
ResultadosDados ={}
local nMax; local nIMax;
local nMin; local nImin;

function onRoll(rSource, rTarget, rRoll)
	local rMessage = ActionsManager.createActionMessage(rSource, rRoll);
	local resultTotal = 0;
	for k,v in ipairs(rMessage.dice) do
		resultTotal = resultTotal + v.result;
	end
	resultTotal=resultTotal+rRoll.nMod;

	Comm.deliverChatMessage(rMessage);

	ResultadosDados[nContDados]=resultTotal;
	if nContDados==1 then
		nMax=resultTotal; nIMax=1;
		nMin=resultTotal; nIMin=1;
	else
		if resultTotal&lt;nMin then nMin=resultTotal; nIMin=nContDados; end
		if resultTotal&gt;nMax then nMax=resultTotal; nIMax=nContDados; end
		if nContDados==4 then
			local Aux={ }; local nContAux=1;
			for i=1,4 do
				if i~=nIMin and i~=nIMax then
					Aux[nContAux]=ResultadosDados[i];
					nContAux=nContAux+1;
				end
			end
			window.BaseCuerpo.setValue(nMax);
			window.BaseEspiritu.setValue(nMin);
			
			if Aux[1]&gt;Aux[2] then window.BaseMente.setValue(Aux[1]);
			else window.BaseMente.setValue(Aux[2]);
			end
		end
	end
	nContDados=nContDados+1;
end

function onInit()
	ActionsManager.registerResultHandler("charwizardabilityroll", onRoll);
	setVisible(window.SeleccionMetodo.getValue()=="Lanzar Dados");
end

function onButtonPress()
	local rRoll = { sType = "charwizardabilityroll", sDesc = "", aDice = aDice, nMod = 0 };	
	rRoll.aDice = {"d6"};
	rRoll.nMod = 3;
	nContDados=1;
	for ContDados=1,4 do
		ActionsManager.performAction(nil,nil,rRoll);
	end
end
</LUACode>
