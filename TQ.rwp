    <LUACode>function onButtonPress()
	window.Modificador.setValue(window.ModificadorAux.getValue());
	local nAmplicarDuracion=0;
	if window.Duracion.getValue()~="Inmediato" then
		if DB.getValue(window.getDatabaseNode(), "...AmplicarDuracion", "Turnos")=="Minutos" then nAmplicarDuracion=1;
		elseif DB.getValue(window.getDatabaseNode(), "...AmplicarDuracion", "Turnos")=="Horas" then nAmplicarDuracion=2;
		end
	end
	local nBlancos=DB.getValue(window.getDatabaseNode(), "...NumBlancos", 1)-1;
	if nBlancos&lt;0 then nBlancos=0; end
	DB.setValue(window.getDatabaseNode(), "...PuntosMagiaGastados", "number", DB.getValue(window.getDatabaseNode(), "...PuntosMagiaGastados", 0) + window.CostePM.getValue()+nBlancos+nAmplicarDuracion);

	local sDificultad=""; local sHabilidad=""; local sSinGestos=""; local sSinHabla=""; local sAcelerado=""; local sDuracion=""; local sCeremonia=""; local sBlancos="";
	
	sDificultad = "[Dificultad "..DB.getValue(window.getDatabaseNode(), "Dificultad", 0).."]";
	
	sHabilidad="["..window.HabilidadLanzamiento.getValue().."]";
	
	if DB.getValue(window.getDatabaseNode(), "...SinGestos", 0)==1 then sSinGestos = "[Sin Gestos: Tirada -2]"; end
	if DB.getValue(window.getDatabaseNode(), "...SinHabla", 0)==1 then sSinHabla = "[Sin Habla: Tirada -3]"; end
	if DB.getValue(window.getDatabaseNode(), "...Acelerado", 0)==1 then sAcelerado = "[Acelerado: Tirada -2]"; end
	
	if DB.getValue(window.getDatabaseNode(), "Duracion", "Inmediato")~="Inmediato" then
		if DB.getValue(window.getDatabaseNode(), "...AmpliarDuracion", "Turnos")=="Minutos" then
			sDuracion="[Duraci√≥n ampliada a minutos: +1 PM]";
		elseif DB.getValue(window.getDatabaseNode(), "...AmpliarDuracion", "Turnos")=="Horas" then
			sDuracion="[Duraci√≥n ampliada a horas: +2 PM]";
		end
	end

	if DB.getValue(window.getDatabaseNode(), "...Ceremonia", "Sin ceremonia")=="1 turno" then
		sCeremonia="[Ceremonia de 1 turno: Tirada +1]";
	elseif DB.getValue(window.getDatabaseNode(), "...Ceremonia", "Turnos")=="10 minutos" then
		sCeremonia="[Ceremonia de 10 minutos: Tirada +2]";
	elseif DB.getValue(window.getDatabaseNode(), "...Ceremonia", "Turnos")=="1 hora" then
		sCeremonia="[Ceremonia de 1 hora: Tirada +3]";
	end
	
	if nBlancos&gt;1 then
		sBlancos="[Blancos incrementados: +"..nBlancos.." PM]";
	end

	window.TextoLanzamiento.setValue(window.name.getValue().." "..sDificultad..sHabilidad..sSinGestos..sSinHabla..sAcelerado..sCeremonia..sBlancos..sDuracion);
	
	
	--Restauramos todos los modificadores de hechizo
	DB.setValue(window.getDatabaseNode(), "...SinHabla", "number", 0);
	DB.setValue(window.getDatabaseNode(), "...SinGestos", "number", 0);
	DB.setValue(window.getDatabaseNode(), "...Acelerado", "number", 0);
	DB.setValue(window.getDatabaseNode(), "...NumBlancos", "number", 1);
	DB.setValue(window.getDatabaseNode(), "...AmpliarDuracion", "string", "Turnos");
	DB.setValue(window.getDatabaseNode(), "...Ceremonia", "string", "Sin ceremonia");
	DB.setValue(window.getDatabaseNode(), "...BonoCeremonia", "number", 0);
end


function onInit()
	window.LanzarHechizo.setTooltipText(window.HabilidadLanzamiento.getValue());	
end
</LUACode>
