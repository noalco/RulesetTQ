    <LUACode>function onValueChanged()
	local nPD=0;
	for _,nodoHeridas in pairs(window.getDatabaseNode().getParent().getChildren()) do
		nPD=nPD+DB.getValue(nodoHeridas, "PD", 0);
	end
	DB.setValue(window.getDatabaseNode().getParent().getParent(), "PD", "number", nPD);
	
	local nGraveIni="-"; local nGraveFin="-"; local nLeveIni="-"; local nLeveFin="-"; local nRasgunioIni="-"; local nRasgunioFin="-";
	nGraveIni, nGraveFin, nLeveIni, nLeveFin, nRasgunioIni, nRasgunioFin=Principal.ObtenerRangos(DB.getValue(window.getDatabaseNode().getParent().getParent(), "VidaMaxima", 10));
	if getValue()&gt;nGraveFin then window.Tipo.setValue("M");
	elseif getValue()&gt;=nGraveIni then window.Tipo.setValue("G");
	elseif getValue()&gt;=nLeveIni then window.Tipo.setValue("L");
	else window.Tipo.setValue("R");
	end
end
</LUACode>
