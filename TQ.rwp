    <LUACode>function onButtonPress()
	local nRasgunioFin=0;
	_, _, _, _, _, nRasgunioFin=Principal.ObtenerRangos(window.VidaMaxima.getValue());
	for _,nodoHerida in pairs(window.ListaHeridas.getDatabaseNode().getChildren()) do
		if DB.getValue(nodoHerida, "Infectada", 0)==0 then
			--Si la herida no est√° infectada, los PD de la herida se reducen en nRasgunioFin
			DB.setValue(nodoHerida, "PD", "number", math.max(DB.getValue(nodoHerida, "PD", 0)-nRasgunioFin, 0));
		else
			--Si la herida no est√° infectada, los PD de la herida se incrementan en nRasgunioFin
			DB.setValue(nodoHerida, "PD", "number", DB.getValue(nodoHerida, "PD", 0)+nRasgunioFin);
		end
	end
end
