    <LUACode>function onButtonPress()
	local bReadOnly = getValue()==1;
	DB.setValue(window.getDatabaseNode(), "locked", "number", getValue());
	for i,v in pairs(window.getControls()) do
		if string.find(v.getName(), "Label") == nil then v.setReadOnly(bReadOnly); end
	end
	for i,v in pairs(window.SubVentanaVentDesvent_main.subwindow.getControls()) do
		if string.find(v.getName(), "Label") == nil then v.setReadOnly(bReadOnly); end
	end
	window.TipoVentDesvent.setEnabled(not bReadOnly);
	window.locked.setReadOnly(false);
	window.ButtonSalir.setReadOnly(false);
end

function onInit()
	setValue(DB.getValue(window.getDatabaseNode(), "locked", 0));
	onButtonPress()
end</LUACode>
