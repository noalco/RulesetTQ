    <LUACode>function onValueChanged()
	local nUmbral=DB.getValue(window.getDatabaseNode(), "Bono", 0)+DB.getValue(window.getDatabaseNode(), "PtosGratis", 0)+DB.getValue(window.getDatabaseNode(), "BonoEtnia", 0)+DB.getValue(window.getDatabaseNode(), "BonoEspecialidad", 0)+DB.getValue(window.getDatabaseNode(), "BonoCulto", 0);
	if getValue()&gt;nUmbral then
		window.BotonLevelUp.setVisible(true);
		DB.setValue(window.getDatabaseNode(), "...HabilidadesPorSubir", "number", 1);
	else
		window.BotonLevelUp.setVisible(false);
		for _,nodoHabilidad in pairs(window.getDatabaseNode().getParent().getChildren()) do
			nUmbral=DB.getValue(nodoHabilidad, "Bono", 0)+DB.getValue(nodoHabilidad, "PtosGratis", 0)+DB.getValue(nodoHabilidad, "BonoEtnia", 0)+DB.getValue(nodoHabilidad, "BonoEspecialidad", 0)+DB.getValue(nodoHabilidad, "BonoCulto", 0);
			if DB.getValue(nodoHabilidad, "Experiencia", 0)&gt;nUmbral then
				DB.setValue(window.getDatabaseNode(), "...HabilidadesPorSubir", "number", 1);
				return;
			end
		end
		DB.setValue(window.getDatabaseNode(), "...HabilidadesPorSubir", "number", 0);
	end
end
