function GuardarHabilidadDeArma(sClave, sSecundario, PathHabilidad)
	if sClave=="Arma CC" or sClave=="Arma Pr" then PathHabilidadesDeArmas[sSecundario]=PathHabilidad;
	elseif sClave=="Pelea" or sClave=="Lanzar" then PathHabilidadesDeArmas[sClave]=PathHabilidad;
	end
end

function NoInsertada(window, sLabel)
	for _,nodoHabilidad in pairs(DB.getChildren(window.getDatabaseNode().getChild("Habilidades"), "ListaHabilidades")) do
		if DB.getValue(nodoHabilidad, "Clave", "")==sLabel then return false; end;
	end;
	return true;
end;

function CrearHabilidades(window)
	for sLabel,_ in pairs(DatosHabilidades) do
		AniadirHabilidad(window, sLabel, false);
	end;
end;

function CrearNombreSecundario(nodoLista, Lista)
	for i=1,#Lista do
		nodoLista.add(Lista[i],Lista[i]);
	end
end

function AniadirHabilidad(window, sLabel, bRepetible)
	--Debug.chat(window)
	local rSkill = DatosHabilidades[sLabel];
	if rSkill and (rSkill.repetible==bRepetible) then 
		local w;
		w = window.ListaHabilidades.createWindow();
		
		w.NombreHabilidad.setValue(rSkill.nombre);
		w.ClaveHabilidad.setValue(sLabel);

		w.Idioma.setVisible(sLabel=="Idioma" or sLabel=="Idioma Nativo");
		w.Idioma.setEnabled(sLabel=="Idioma" or sLabel=="Idioma Nativo");
		w.Artesania.setVisible(sLabel=="Artesania");
		w.Artesania.setEnabled(sLabel=="Artesania");
		w.HabilidadCC.setVisible(sLabel=="Arma CC");
		w.HabilidadCC.setEnabled(sLabel=="Arma CC");
		w.HabilidadPr.setVisible(sLabel=="Arma Pr");
		w.HabilidadPr.setEnabled(sLabel=="Arma Pr");
		w.idelete.setVisible(rSkill.repetible);
		
		DB.setValue(w.getDatabaseNode(), "PtosGratis", "number", rSkill.gratuito);
		--w.PathBaseHabilidad.setValue("...."..PathHabilidades[rSkill.base]);
		w.BaseHabilidad.setValue(rSkill.base);
		w.MultEstorbo.setValue(rSkill.multEstorbo);
		
		if sLabel=="Pelea" or sLabel=="Lanzar" then
			DB.setValue(w.getDatabaseNode().getParent().getParent(), "Path"..sLabel, "string", ".Habilidades.ListaHabilidades."..w.getDatabaseNode().getName()..".Total");
		end
	end
end

function CalcularPuntosTotalesHabilidades(window)
	local nTotalPuntosHabilidad=0;
	local nTotalPuntosHabilidadNoProfesion=0;
	for _,nodoHabilidad in pairs(window.getDatabaseNode().getParent().getChildren("ListaHabilidades")) do
		if DB.getValue(nodoHabilidad, "DeProfesion", 0)==1 then	nTotalPuntosHabilidad=nTotalPuntosHabilidad+DB.getValue(nodoHabilidad, "Bono", 0);
		else nTotalPuntosHabilidadNoProfesion=nTotalPuntosHabilidadNoProfesion+DB.getValue(nodoHabilidad, "Bono", 0);
		end
	end;
	DB.setValue(window.getDatabaseNode(), "...PuntosHabilidad", "number", nTotalPuntosHabilidad);
	DB.setValue(window.getDatabaseNode(), "...PuntosHabilidadNoProfesion", "number", nTotalPuntosHabilidadNoProfesion);
end;
