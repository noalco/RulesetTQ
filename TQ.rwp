    <LUACode>function onInit()
	setVisible(window.HabilidadesPorSubir.getValue()==1);
end

function onButtonPress()
	window.HabilidadesPorSubir.setValue(0);
	setVisible(false);
	for _,nodoHabilidad in pairs(window.getDatabaseNode().getChild("Habilidades").getChild("ListaHabilidades").getChildren()) do
		local nUmbral=DB.getValue(nodoHabilidad, "Bono", 0)+DB.getValue(nodoHabilidad, "PtosGratis", 0)+DB.getValue(nodoHabilidad, "BonoEtnia", 0)+DB.getValue(nodoHabilidad, "BonoEspecialidad", 0)+DB.getValue(nodoHabilidad, "BonoCulto", 0);
		if DB.getValue(nodoHabilidad, "Experiencia", 0)&gt;nUmbral then
			DB.setValue(nodoHabilidad, "Bono", "number", DB.getValue(nodoHabilidad, "Bono", 0)+1);
			DB.setValue(nodoHabilidad, "Experiencia", "number", DB.getValue(nodoHabilidad, "Experiencia", 0)-nUmbral-1);
		end
	end	
end
</LUACode>
