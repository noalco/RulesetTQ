function onValueChanged()
	super.onValueChanged();

	-- This template gets rid of the relative positioning of the headers, 
	-- so their position must be adjusted manually when the delete button is shown and hidden.	
	if getValue() == 1 then
		window.location_label.setStaticBounds(-253, 36, 80, 20);
		window.weight_label.setStaticBounds(-160, 36, 30, 20);
		window.carried_header.setStaticBounds(-121, 36, 20, 20);
	else
		window.location_label.setStaticBounds(-233, 36, 80, 20);
		window.weight_label.setStaticBounds(-140, 36, 30, 20);
		window.carried_header.setStaticBounds(-101, 36, 20, 20);
	end
end
