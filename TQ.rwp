    <LUACode>function Actualizar(sRecord)
	local sPathNombre="";
	if string.find(sRecord, "@") then
		sPathName, _=string.gsub(sRecord, "@", ".name@");
	else
		sPathName=sRecord..".name";
	end
	setValue(DB.getValue(sPathName, ""));
end

function onDrop(x, y, dragdata)
	local sClass,sRecord = dragdata.getShortcutData();
	if sClass=="VentanaEtniaSidebar" then
		window.LinkEtnia.onDrop(x, y, dragdata);
		Actualizar(sRecord);
	end;
end
