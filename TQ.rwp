    <LUACode>function onInit()
	window.NombreTipoObjeto.setValue(Inventario.CadenasTipoObjeto[getValue()]);

	window.BordeBotonMisc.setVisible(getValue()==Inventario.CteObjeto);
	window.BordeBotonLibro.setVisible(getValue()==Inventario.CteLibro);
	window.BordeBotonPocionVeneno.setVisible(getValue()==Inventario.CtePocionVeneno);
	window.BordeBotonArma.setVisible(getValue()==Inventario.CteArma);
	window.BordeBotonEscudo.setVisible(getValue()==Inventario.CteEscudo);
	window.BordeBotonArmadura.setVisible(getValue()==Inventario.CteArmadura);
	window.BordeBotonYelmo.setVisible(getValue()==Inventario.CteYelmo);
	nAncho, nAlto= window.parentcontrol.window.getSize();
	if nAncho&lt;350 then nAncho=350 end

	if getValue()==Inventario.CteArma and nAlto &lt;610 then nAlto=610;
	elseif getValue()==Inventario.CteArmadura and nAlto&lt;490 then nAlto=490;
	elseif getValue()==Inventario.CteEscudo and nAlto&lt;640 then nAlto=640;
	elseif getValue()==Inventario.CteYelmo and nAlto&lt;490 then nAlto=490;
	elseif getValue()==Inventario.CteLibro and nAlto&lt;460 then nAlto=460;
	elseif getValue()==Inventario.CtePocionVeneno and nAlto&lt;660 then nAlto=660;
	elseif getValue()==Inventario.CteObjeto and nAlto&lt;400 then nAlto=400;
	end
		
	window.parentcontrol.window.setSize(nAncho, nAlto);
	window.SeleccionObjeto.activateTab(getValue());
end

function onValueChanged()
	window.NombreTipoObjeto.setValue(Inventario.CadenasTipoObjeto[getValue()]);
	onInit();
end
