    <LUACode>function Actualizar()
	local nUsoArma=DB.getValue(window.getDatabaseNode(), "UsoArma", Inventario.CteUnaMano);
	if nUsoArma==Inventario.CteUnaMano or nUsoArma==Inventario.CteArrojadiza or nUsoArma==Inventario.CteRangoUnaMano then
		window.PenalizacionPorFuerzaInsuficiente.setValue(math.min(window.Fuerza.getValue()-window.FuerzaMinima.getValue(), 0));
	else
		if window.ManoIzquierda.getValue()~=window.ManoDerecha.getValue() then
			window.PenalizacionPorFuerzaInsuficiente.setValue(math.min(window.Fuerza.getValue()-2*window.FuerzaMinima.getValue(), 0));
		else
			window.PenalizacionPorFuerzaInsuficiente.setValue(math.min(window.Fuerza.getValue()-window.FuerzaMinima.getValue(), 0));
		end;
	end
end

function onInit()
	Actualizar();
end

function MensajeFuerzaInsuficiente()
	if getValue()== 0 then return "";
	else return "[Penalizaci√≥n por Fuerza insuficiente: "..getValue().."]";
	end
end</LUACode>
