function Actualizar()
	if window.SeleccionMetodo.getValue()=="Al azar" then
	-- Si caracter√≠sticas al azar
		window.MaxCuerpo.setValue(9);
		setValue(window.PtosDisponibles.getValue());
		-- Modificaci√≥n en funci√≥n de la edad
		if window.Edad.getValue()&lt;=15 then		-- Ni√±o: Cuerpo -1, Mente -1
			window.ModEdadCuerpo.setValue(-1);
			window.ModEdadMente.setValue(-1);
		elseif window.Edad.getValue()&lt;=20 then	-- Joven: Mente -1
			window.ModEdadCuerpo.setValue(0);
			window.ModEdadMente.setValue(-1);
		elseif window.Edad.getValue()&lt;=35 then	-- Adulto: Sin penalizadores
			window.ModEdadCuerpo.setValue(0);
			window.ModEdadMente.setValue(0);
		elseif window.Edad.getValue()&lt;=49 then	-- Mayor: Cuerpo -1
			window.ModEdadCuerpo.setValue(-1);
			window.ModEdadMente.setValue(0);
		elseif window.Edad.getValue()&lt;=64 then	-- Viejo: Cuerpo -2
			window.ModEdadCuerpo.setValue(-2);
			window.ModEdadMente.setValue(0);
		else									-- Anciano: Cuerpo -3 Mente -1
			window.ModEdadCuerpo.setValue(-3);
			window.ModEdadMente.setValue(-1);
		end

		-- Modificaci√≥n en funci√≥n de la especie
		if window.SeleccionEspecie.getValue()=="Mestizo" then
			-- Mestizo: +1 a Esp√≠ritu
			window.ModEspecieEspiritu.setValue(1);
			window.ModEspecieMente.setValue(0);
		elseif window.SeleccionEspecie.getValue()=="Humano" then
			-- Humano: Sin bonos
			window.ModEspecieEspiritu.setValue(0);
			window.ModEspecieMente.setValue(0);
		else
			-- Mener√≠: +1 a Mente y Esp√≠ritu
			window.ModEspecieEspiritu.setValue(1);
			window.ModEspecieMente.setValue(1);
		end
	else
	-- Si reparto de puntos
		window.ModEdadCuerpo.setValue(0);
		window.ModEdadMente.setValue(0);
		window.ModEspecieEspiritu.setValue(1);
		window.ModEspecieMente.setValue(0);

		-- Modificaci√≥n en funci√≥n de la edad
		if window.Edad.getValue()&lt;=15 then		-- Ni√±o: 17 puntos y MaxCuerto 7
			setValue(17);
			window.MaxCuerpo.setValue(7);
		elseif window.Edad.getValue()&lt;=20 then	-- Joven: 18 puntos
			setValue(18);
			window.MaxCuerpo.setValue(9);
		elseif window.Edad.getValue()&lt;=35 then	-- Adulto: Sin bonos
			setValue(19);
			window.MaxCuerpo.setValue(9);
		elseif window.Edad.getValue()&lt;=49 then	-- Mayor: 18 puntos y MaxCuerpo 8
			setValue(18);
			window.MaxCuerpo.setValue(8);
		elseif window.Edad.getValue()&lt;=64 then	-- Viejo: 17 puntos y MaxCuerpo 7
			setValue(17);
			window.MaxCuerpo.setValue(7);
		else									-- Anciano: 15 puntos y MaxCuerpo 6
			setValue(15);
			window.MaxCuerpo.setValue(6);
		end

		-- Modificaci√≥n en funci√≥n de la especie
		--Meren√≠ +2 puntos a repartir, Mestizo +1 punto a repartir
		--setValue(getValue()+tonumber(window.SeleccionEspecie.getValue()));
		-- Modificaci√≥n en funci√≥n de la especie
		if window.SeleccionEspecie.getValue()=="Mestizo" then
			setValue(getValue()+1);
		elseif window.SeleccionEspecie.getValue()=="Mereni" then
			setValue(getValue()+2);
		end
		window.ModEspecieEspiritu.setValue(0);
		window.ModEspecieMente.setValue(0);
	end
end

function onInit()
	Actualizar();
end
</LUACode>
