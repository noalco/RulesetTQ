    <LUACode>sMensajeTirada="tirada";

function ObtenerTirada()
	local sDado;
	if DB.getValue(window.getDatabaseNode(), "...Debilitado", 0)==1 then sDado="1d6!";
	else sDado="1d10!";
	end
	local sVerbo=DB.getValue(window.getDatabaseNode(), "Verbo", "");
	local sYVerbo=DB.getValue(window.getDatabaseNode(), "YVerbo", "");
	local sEsfera=DB.getValue(window.getDatabaseNode(), "Esfera", "");
	local sEnEsfera=DB.getValue(window.getDatabaseNode(), "EnEsfera", "");
	sHabilidades=DB.getValue(window.getDatabaseNode(), "...HabilidadesMagicas", "");
	local nVerbo=10000; local nYVerbo=10000; local nEsfera=10000; local nEnEsfera=10000; 
	if sHabilidades~="" then
		if sVerbo~="" then
			nVerbo=sHabilidades:match(sVerbo.." (%d*)");
			if nVerbo==nil then nVerbo=10000; else nVerbo=tonumber(nVerbo); end
		else nVerbo=10000;
		end
		
		if sYVerbo~="" then
			nYVerbo=sHabilidades:match(sYVerbo.." (%d*)");
			if nYVerbo==nil then nYVerbo=10000; else nYVerbo=tonumber(nYVerbo); end
		else nYVerbo=10000;
		end
		
		if sEsfera~="" then
			nEsfera=sHabilidades:match(sEsfera.." (%d*)");
			if nEsfera==nil then nEsfera=10000; else nEsfera=tonumber(nEsfera); end
		else nEsfera=10000;
		end
		
		if sEnEsfera~="" then
			nEnEsfera=sHabilidades:match(sEnEsfera.." (%d*)");
			if nEnEsfera==nil then nEnEsfera=10000; else nEnEsfera=tonumber(nEnEsfera); end
		else nEnEsfera=10000;
		end
	end
	
	local nMod=math.min(nVerbo, nYVerbo, nEsfera, nEnEsfera);
	if nMod==10000 then nMod=window.TiradaFija.getValue(); end
	
	sMensajeTirada=window.name.getValue();
	if nMod==nVerbo then sMensajeTirada=sMensajeTirada.." ["..sVerbo.."]";
	elseif nMod==nYVerbo then sMensajeTirada=sMensajeTirada.." ["..sYVerbo.."]";
	elseif nMod==nEsfera then sMensajeTirada=sMensajeTirada.." ["..sEsfera.."]";
	elseif nMod==nEnEsfera then sMensajeTirada=sMensajeTirada.." ["..sEnEsfera.."]";
	end
	
	local sCadena="";
	if nMod==0 then sCadena=sDado;
	elseif nMod&gt;0 then sCadena=sDado.."+"..nMod;
	else sCadena=sDado..nMod;
	end
	
	DB.setValue(window.getDatabaseNode(), "...PMGastados", "number", DB.getValue(window.getDatabaseNode(), "...PMGastados", 0)+DB.getValue(window.getDatabaseNode(), "CostePM", 0));
	
	return sCadena;
end</LUACode>
