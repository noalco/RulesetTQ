    <LUACode>function ObtenerTirada()
	local sDado;
	if DB.getValue(window.getDatabaseNode(), "...Debilitado", 0)==1 then sDado="1d6!";
	else sDado="1d10!";
	end
	local nEspiritu=DB.getValue(window.getDatabaseNode(), "...Espiritu", 0);
	if nEspiritu==0 then return sDado;
	elseif nEspiritu&gt;0 then return sDado.."+"..nEspiritu;
	else return sDado..nEspiritu;
	end
end</LUACode>
