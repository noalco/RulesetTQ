    <LUACode>function onValueChanged()
	Principal.ActualizarBonosDanio(window.getDatabaseNode());
	local nCarga=DB.getValue(window.getDatabaseNode(), "encumbranceload", 0);
	local nFuerza=getValue();
	if nFuerza==0 then
		DB.setValue(window.getDatabaseNode(), ".Inventario.Estorbo", "number",  nCarga);
		--window.Estorbo.setValue(nCarga);
	else
		DB.setValue(window.getDatabaseNode(), ".Inventario.Estorbo", "number",  math.floor(nCarga/nFuerza));
	end
end;

function onInit()
	onValueChanged();
end</LUACode>
