    <LUACode>function onButtonPress()
	bMostrar=DB.getValue(window.getDatabaseNode(), ".Hechiceria.MostrarContadores", 0)==1;
	window.LabelDeProfesionGastados.setVisible(not bMostrar);
	window.LabelDeNoProfesionGastados.setVisible(not bMostrar);
	window.LabelDeProfesionDisponibles.setVisible(not bMostrar);
	window.LabelDeNoProfesionDisponibles.setVisible(not bMostrar);
	window.LabelPuntosHabilidadProfesion.setVisible(not bMostrar);
	window.PuntosHabilidad.setVisible(not bMostrar);
	window.PuntosHabilidadNoProfesion.setVisible(not bMostrar);
	window.LabelPtosHabilidadDisponible.setVisible(not bMostrar);
	window.NumHabilidadesProfDisponibles.setVisible(not bMostrar);
	window.NumHabilidadesNoProfDisponibles.setVisible(not bMostrar);
	if bMostrar then
		DB.setValue(window.getDatabaseNode(), ".Hechiceria.MostrarContadores", "number", 0);
		setText("Mostrar Contadores", "");
--		setFrame("buttonup");
	else
		DB.setValue(window.getDatabaseNode(), ".Hechiceria.MostrarContadores", "number", 1);
		setText("Ocultar Contadores", "")
--		setFrame("buttondown");
	end
end

function onInit()
	bMostrar=DB.getValue(window.getDatabaseNode(), ".Hechiceria.MostrarContadores", 0)==1;
	window.LabelDeProfesionGastados.setVisible(bMostrar);
	window.LabelDeNoProfesionGastados.setVisible(bMostrar);
	window.LabelDeProfesionDisponibles.setVisible(bMostrar);
	window.LabelDeNoProfesionDisponibles.setVisible(bMostrar);
	window.LabelPuntosHabilidadProfesion.setVisible(bMostrar);
	window.PuntosHabilidad.setVisible(bMostrar);
	window.PuntosHabilidadNoProfesion.setVisible(bMostrar);
	window.LabelPtosHabilidadDisponible.setVisible(bMostrar);
	window.NumHabilidadesProfDisponibles.setVisible(bMostrar);
	window.NumHabilidadesNoProfDisponibles.setVisible(bMostrar);
end</LUACode>
