    <LUACode>function onInit()
	OptionsManager.registerCallback("CTSI", onOptionCTSIChanged);
	self.onOptionCTSIChanged();
	OptionsManager.registerCallback("MVPNJ", updateHealthDisplay);
	updateHealthDisplay();
	
	CrearSeccionFase(0);
	CrearSeccionFase(1);
	CrearSeccionFase(2);
	CrearSeccionFase(3);
	CrearSeccionFase(4);
	
end

function updateHealthDisplay()
	for _,w in pairs(list.getWindows()) do
		w.updateHealthDisplay();
	end
end

function onClose()
	OptionsManager.unregisterCallback("CTSI", onOptionCTSIChanged);
end

function onOptionCTSIChanged()
	if label_init then
		local bShowInit = not OptionsManager.isOption("CTSI", "off");
		label_init.setVisible(bShowInit);
	end
	if list and list.onOptionCTSIChanged then
		list.onOptionCTSIChanged();
	end
end

function CrearSeccionFase(numFase)
	local nodeWin = getDatabaseNode();
	local Encabezado = DB.createChild(nodeWin, "fase" .. numFase);
	local nFase = DB.createChild(Encabezado, "Fase", "number");
	nFase.setValue(numFase -0.5);
--	local faction = DB.createChild(Encabezado, "friendfoe", "string");
--	faction.setValue('neutral');
	win = list.createWindowWithClass("EncabezadoFase", Encabezado);
	if numFase == 0 then
        win.txtFase.setValue('Declaración de intenciones');
        else  win.txtFase.setValue('Fase ' .. numFase);
    end
end</LUACode>
