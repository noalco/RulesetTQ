    <LUACode>function onButtonPress()
    local aEntries = CombatManager.getSortedCombatantList();
	local nodeActive = CombatManager.getActiveCT();
	local bEsUltimo=nodeActive==aEntries[#aEntries];

    CombatManager.notifyEndTurn();
    
	if bEsUltimo then
		CT.NuevoTurno(window);
		ResolucionCombate.LimpiarMeles();
	end
end

function onDragStart(button, x, y, draginfo)
    draginfo.setType("combattrackernextactor");
    draginfo.setIcon("button_ctnextactor");
    
    return true;
end</LUACode>
