end

function onInit()
	window.onSizeChanged = handleSizeChanged;
	if window.getDatabaseNode().getChild("Habilidades")==nil or
	   window.getDatabaseNode().getChild("Habilidades").getChild("ListaHabilidades")==nil or
	   window.getDatabaseNode().getChild("Habilidades").getChild("ListaHabilidades").getChildCount()==0 then
		Habilidades.CrearHabilidades(window);
	end;
end


