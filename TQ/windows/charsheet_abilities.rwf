    <LUACode>function onInit()
	local nFuerza=DB.getValue(window.getDatabaseNode(),"Fuerza", 0);
	if nFuerza==0 then
		setValue(0);
	else
		setValue(math.floor(DB.getValue(window.getDatabaseNode(),"encumbranceload", 0)/nFuerza));
	end
end

function onValueChanged()
	for _,w in pairs (window.ListaHabilidades.getWindows()) do
		w.EstorboTotal.Actualizar();
	end
end
