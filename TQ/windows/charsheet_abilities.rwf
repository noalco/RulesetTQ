	for _,w in ipairs(window.ListaHabilidades.getWindows()) do
		if w.ClaveHabilidad.getValue()=="Arma CC" or w.ClaveHabilidad.getValue()=="Arma Pr" or w.ClaveHabilidad.getValue()=="Artesania" or w.ClaveHabilidad.getValue()=="Idioma" then
			w.idelete.setVisibility(bEditMode);
		else
			w.idelete.setVisibility(false);
		end
	end
end

function onInit()
	onValueChanged();
end
