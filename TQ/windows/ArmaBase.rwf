    <LUACode>function onInit()
	addSourceWithOp("BonoHabilidadAtaque", "+");
	addSourceWithOp("BonoAtaqueArma", "+");
	addSourceWithOp("PenalizacionPorFuerzaInsuficiente", "+");
		
	super.onInit();
	DB.addHandler(getDatabaseNode().getPath(), "onUpdate", ActualizarBonoAtaque);
end

function ActualizarBonoAtaque(nodo)
	if window.ManoIzquierda.getValue()==1 and window.ManoDerecha.getValue()==1 then
		Combate.BlandirArma(window, "Izquierda");
		Combate.BlandirArma(window, "Derecha");
	elseif window.ManoIzquierda.getValue()==1 then
		Combate.BlandirArma(window, "Izquierda");
	elseif window.ManoDerecha.getValue()==1 then
		Combate.BlandirArma(window, "Derecha");
	end
end
