function TipoDanio()
	sTipo=DB.getValue(window.getDatabaseNode(), "TipoArma", "");
	if sTipo=="2" then return "Penetrante";
	elseif sTipo=="1" then return "Cortante";
	elseif sTipo=="3" then return "Contundente";
	elseif sTipo=="4" then return "Desgarrador";
	else return "";
	end
end</LUACode>
