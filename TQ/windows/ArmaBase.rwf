    <LUACode>function onClickRelease(button, x, y)
	if window.UnaDosManos.getValue()==1 then
		if DB.getValue(window.getDatabaseNode(), "UsoArma", "")==Inventario.CteUnaMano then
			DB.setValue(window.getDatabaseNode(), "UsoArma", "string", Inventario.CteDosManos);
		elseif DB.getValue(window.getDatabaseNode(), "UsoArma", "")==Inventario.CteDosManos then
			DB.setValue(window.getDatabaseNode(), "UsoArma", "string", Inventario.CteUnaMano);
		end
	end
end

function CambiarUsoArma(nodo)
	if DB.getValue(window.getDatabaseNode(), "UsoArma", "")==Inventario.CteUnaMano or DB.getValue(window.getDatabaseNode(), "UsoArma", "")==Inventario.CteArrojadiza then
		window.ModDanio.setValue(window.Mod1M.getValue());
	elseif DB.getValue(window.getDatabaseNode(), "UsoArma", "")==Inventario.CteDosManos or DB.getValue(window.getDatabaseNode(), "UsoArma", "")==Inventario.CteArrojadizaDosManos then
		window.ModDanio.setValue(window.Mod2M.getValue());
	else
		window.ModDanio.setValue(0);
	end
	window.ManoIzquierda.setValue(0);
	window.ManoDerecha.setValue(0);
--	Combate.ActivarArma(window.getDatabaseNode(), DB.getValue(window.getDatabaseNode(), "...ArmaEnDerecha", ""), DB.getValue(window.getDatabaseNode(), "...ArmaEnIzquierda", ""));
end

function onInit()
	super.onInit();
	Combate.ActivarArma(window.getDatabaseNode(), DB.getValue(window.getDatabaseNode(), "...ArmaEnDerecha", ""), DB.getValue(window.getDatabaseNode(), "...ArmaEnIzquierda", ""));
	DB.addHandler(window.getDatabaseNode().getChild("UsoArma").getPath(), "onUpdate", CambiarUsoArma);
end
