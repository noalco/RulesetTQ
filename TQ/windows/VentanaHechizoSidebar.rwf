    <LUACode>function CrearSintaxis()
	sSintaxis=window.Verbo.getValue();
	if window.YVerbo.getValue()~="" then
		if string.lower(string.sub(window.YVerbo.getValue(), 1, 1))=="i" then
			sSintaxis=sSintaxis.." e "..window.YVerbo.getValue();
		else
			sSintaxis=sSintaxis.." y "..window.YVerbo.getValue();
		end
	end
	if window.Esfera.getValue()~="" then sSintaxis=sSintaxis.." "..window.Esfera.getValue(); end
	if window.EnEsfera.getValue()~="" then sSintaxis=sSintaxis.." en "..window.EnEsfera.getValue(); end
	setValue(sSintaxis)
end

function onInit()
	CrearSintaxis();
end</LUACode>
