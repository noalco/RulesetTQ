	end
	window.Verbo.setEnabled(not bReadOnly);
	window.Esfera.setEnabled(not bReadOnly);
	window.EnEsfera.setEnabled(not bReadOnly);
	window.locked.setReadOnly(false);
	window.ButtonSalir.setReadOnly(false);
end

function onInit()
	onButtonPress()
end</LUACode>
