function onInit()
	update();
	onValueChanged();
end

function onValueChanged()
	if getValue()&gt;0 and OptionsManager.getOption("MMEF")=="on" then
		Herramientas.Mensaje(window.name.getValue().." actúa en Fase "..getValue());
	end
	window.SelectorMele.setVisible(getValue()==2);
	window.SelectorMele3.setVisible(getValue()==3);
	window.labelMele.setVisible(getValue()==2 or getValue()==3);
	window.Mente.setVisible(getValue()==0)
	window.Agilidad.setVisible(getValue()~=0)
end
