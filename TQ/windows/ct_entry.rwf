function onValueChanged()
	local nEstado=getValue()/window.hp.getValue();
	local sColor = ColorManager.getTieredHealthColor(1-nEstado);
	window.wounds.setColor(sColor);
	window.current.setColor(sColor);
	local sTipo, sRuta=window.link. getValue();
	window.idelete.setVisible(getValue()&lt;=0 and sTipo=="npc");
