    <LUACode>function MostrarCamposCombCCArrojadiza(bMostrar)
	window.LabelDanioCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	window.BonoDanioCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	window.DanioDadosCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	
	window.LabelEspecialCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	window.EfectoDanioCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	
	window.LabelTipoArmaCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	window.TipoArmaCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	
	window.LabelLongitudCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
	window.LongitudCombCCArrojadiza.setVisible(bMostrar and getValue()==1);
end

function onValueChanged()
	MostrarCamposCombCCArrojadiza(true);
	Inventario.ModificarArma(window);
end

