    <LUACode>function onValueChanged()
	local sUsoArma=DB.getValue(window.getDatabaseNode(), "UsoArma", "");
	if sUsoArma==Inventario.CteUnaMano or sUsoArma==Inventario.CteDosManos or sUsoArma==Inventario.CteUnaDosManos then
		window.HabilidadCC.setVisible(true);
		window.HabilidadPr.setVisible(false);
		window.HabilidadAr.setVisible(false);
		window.LabelLongAlc.setValue("Longitud");
		window.Longitud.setVisible(true);
		window.Alcance.setVisible(false);
		window.LabelRecarga.setVisible(false);
		window.Recarga.setVisible(false);
		window.LabelCombCCArrojadiza.setVisible(true);
		window.CombCCArrojadiza.setVisible(true);
		window.CombCCArrojadiza.MostrarCamposCombCCArrojadiza(true);
	elseif sUsoArma==Inventario.CteRango or sUsoArma==Inventario.CteRangoUnaMano then
		window.HabilidadCC.setVisible(false);
		window.HabilidadPr.setVisible(true);
		window.HabilidadAr.setVisible(false);
		window.LabelLongAlc.setValue("Alcance");
		window.Longitud.setVisible(false);
		window.Alcance.setVisible(true);
		window.LabelRecarga.setVisible(true);
		window.Recarga.setVisible(true);
		window.LabelCombCCArrojadiza.setVisible(false);
		window.CombCCArrojadiza.setVisible(false);
		window.CombCCArrojadiza.MostrarCamposCombCCArrojadiza(false);
	elseif sUsoArma==Inventario.CteArrojadiza or sUsoArma==Inventario.CteArrojadizaDosManos then
		window.HabilidadCC.setVisible(false);
		window.HabilidadPr.setVisible(false);
		window.HabilidadAr.setVisible(true);
		window.LabelLongAlc.setValue("Alcance");
		window.Longitud.setVisible(false);
		window.Alcance.setVisible(true);
		window.LabelRecarga.setVisible(false);
		window.Recarga.setVisible(false);
		window.LabelCombCCArrojadiza.setVisible(false);
		window.CombCCArrojadiza.setVisible(false);
		window.CombCCArrojadiza.MostrarCamposCombCCArrojadiza(false);
	end
	
	Inventario.ModificarArma(window);
	if sUsoArma==Inventario.CteUnaDosManos then
		DB.setValue(window.IDListaArmas.getValue()..".UsoArma", "string", Inventario.CteUnaMano);
	else
		DB.setValue(window.IDListaArmas.getValue()..".UsoArma", "string", sUsoArma);
	end
end
</LUACode>
