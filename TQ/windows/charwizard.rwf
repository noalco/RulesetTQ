    <LUACode>function onButtonPress()
	local bEsCorrecto, sMensaje=ChequeoPJ.ComprobarCorrecto(window);
	if bEsCorrecto then
		window.Mensajes.setVisible(false);
		window.Mensajes2.setVisible(true);
		window.Mensajes2.setValue("&lt;p&gt;Tu personaje es correcto.&lt;/p&gt;");
		window.onCommitButtonPressed(window);
	else
		window.Mensajes.setVisible(false);
		window.Mensajes2.setVisible(true);
		window.Mensajes2.setValue(Informacion.CadenasInfo["CabeceraErrores"]..sMensaje);
	end
--	wPostWizard=Interface.openWindow("VentanaPostWizard", "");
--	PostWizard.GenerarMensaje(wPostWizard, window);
end</LUACode>
