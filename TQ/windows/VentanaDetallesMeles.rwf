--							sTablaMele=sTablaMele..sTab6.."&lt;td&gt;"..ResolucionCombate.tResultados[tAliados[i].PathNodo][tEnemigos[j].PathNodo].."&lt;/td&gt;";
--						elseif ResolucionCombate.tResultados and (ResolucionCombate.tResultados[tEnemigos[j].PathNodo] and ResolucionCombate.tResultados[tEnemigos[j].PathNodo][tAliados[i].PathNodo]) then
--							sTablaMele=sTablaMele..sTab6.."&lt;td&gt;-"..ResolucionCombate.tResultados[tEnemigos[j].PathNodo][tAliados[i].PathNodo].."&lt;/td&gt;";
--						else
--							sTablaMele=sTablaMele..sTab6.."&lt;td /&gt;";
