--		tAliados={}
--		tEnemigos={}
--		for _,Integrante in pairs(tMele) do
--			if Integrante.bEsAliado then
--				table.insert(tAliados, Integrante)
--			else
--				table.insert(tEnemigos, Integrante)
--			end
--		end
--		
--		-- Cabecera de la tabla con los enemigos
--		if #tEnemigos &gt; 0 then
--			sTablaMele=sTablaMele..sTab5.."&lt;tr&gt;";
--			
--			if #tAliados&gt;0 then sTablaMele=sTablaMele..sTab6.."&lt;td /&gt;"; end
--			
--			sTablaMele=sTablaMele..sTab6.."&lt;td colspan=\""..#tEnemigos.."\"&gt;Bando 2&lt;/td&gt;"..sTab5.."&lt;/tr&gt;"..sTab5.."&lt;tr&gt;";
--			
--			if #tAliados&gt;0 then sTablaMele=sTablaMele..sTab6.."&lt;td&gt;Bando 1&lt;/td&gt;"; end
--			
--			for i=1,#tEnemigos do
--				sTablaMele=sTablaMele..sTab6.."&lt;td&gt;"..DB.getValue(tEnemigos[i].PathNodo..".name", "");
--				if tEnemigos[i].bTirada then sTablaMele=sTablaMele.." ("..tEnemigos[i].nTirada..")"; end
--				sTablaMele=sTablaMele.."&lt;/td&gt;";
