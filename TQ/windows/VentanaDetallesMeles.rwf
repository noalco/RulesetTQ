		if ResolucionCombate.tContadores[i].nAliados&gt;0 or ResolucionCombate.tContadores[i].nEnemigos&gt;0 then
			tAliados={}
			tEnemigos={}
			for _,Integrante in pairs(tMele) do
				if Integrante.bEsAliado then
					table.insert(tAliados, Integrante)
				else
					table.insert(tEnemigos, Integrante)
				end
			end
			sTablaMele=sTablaMele..sTab4.."&lt;h&gt;Melé "..i.."&lt;/h&gt;";
			if #tAliados&gt;0 then
				sTablaMele=sTablaMele..sTab4.."&lt;p&gt;&lt;b&gt;Bando 1:&lt;/b&gt;&lt;/p&gt;"..sTab4.."&lt;list&gt;";
				for i=1,#tAliados do
					sTablaMele=sTablaMele..sTab5.."&lt;li&gt;"..DB.getValue(tAliados[i].PathNodo..".name", "");
					if tAliados[i].bTirada then sTablaMele=sTablaMele.." - Resultado de su tirada: "..tAliados[i].nTirada; end
					if #tEnemigos&gt;1 then
						sTablaMele=sTablaMele.." - Penalizador por múltiples enemigos: -"..DB.getValue(tAliados[i].PathNodo..".PenMeleMultiEnemigos", "");
					end
					sTablaMele=sTablaMele..sTab5.."&lt;/li&gt;"
				end
				sTablaMele=sTablaMele..sTab4.."&lt;/list&gt;";
			end
			if #tEnemigos&gt;0 then
				sTablaMele=sTablaMele..sTab4.."&lt;p&gt;&lt;b&gt;Bando 2:&lt;/b&gt;&lt;/p&gt;"..sTab4.."&lt;list&gt;";
				for i=1,#tEnemigos do
					sTablaMele=sTablaMele..sTab5.."&lt;li&gt;"..DB.getValue(tEnemigos[i].PathNodo..".name", "");
					if tEnemigos[i].bTirada then sTablaMele=sTablaMele.." - Resultado de su tirada: "..tEnemigos[i].nTirada; end
					if #tAliados&gt;1 then
						sTablaMele=sTablaMele.." - Penalizador por múltiples enemigos: -"..DB.getValue(tEnemigos[i].PathNodo..".PenMeleMultiEnemigos", "");
					end
					sTablaMele=sTablaMele..sTab5.."&lt;/li&gt;"
				end
				sTablaMele=sTablaMele..sTab4.."&lt;/list&gt;";
			end
			
			sTablaMele=sTablaMele..sTab4.."&lt;p&gt;Tiradas realizadas por los integrantes de la melé: "..ResolucionCombate.tContadores[i].nAtaques.."&lt;/p&gt;";
			
			if ResolucionCombate.tContadores[i].nAliados&gt;1 and ResolucionCombate.tContadores[i].nEnemigos&gt;1 then
				sTablaMele=sTablaMele..sTab4.."&lt;p&gt;El número de integrantes de la melé no es válida. Reasigna algunos integrantes en otra melé.&lt;/p&gt;";
			end
			
	--		Debug.chat(sTablaMele)
		end
	end
    
--    DB.setValue("combattracker.Meles", "formattedtext", sTablaMele);
	setValue(sTablaMele);
end
