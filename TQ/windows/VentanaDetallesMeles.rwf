--				sTablaMele=sTablaMele..sTab5.."&lt;tr&gt;"..sTab6.."&lt;td&gt;"..DB.getValue(tAliados[i].PathNodo..".name", "");
--				if tAliados[i].bTirada then sTablaMele=sTablaMele.." ("..tAliados[i].nTirada..")"; end
