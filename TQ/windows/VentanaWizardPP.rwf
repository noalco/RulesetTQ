function onDrop(x, y, dragdata)
	local sClass,sRecord = dragdata.getShortcutData();
	if sClass=="VentanaVentDesventSidebar" then
		local _,sRecordAnt = getValue();
		ParseoVentDesv.ModificarHabilidades(sRecordAnt, "VentDes", false);
		ParseoVentDesv.ModificarBaseDeHabilidades(sRecordAnt, false);
		
		window.parentcontrol.window.Mensajes.setValue("");
		setValue(dragdata.getShortcutData());
		window.NombreVentDesv1.Actualizar(sRecord);
		window.ValorVentDesv1.Actualizar(sRecord);
		ParseoVentDesv.PorEleccion[1].sNombre=Utilidades.LeerValorRegistro(sRecord, "name");
		ParseoVentDesv.PorEleccion[1].sLink=sRecord;
		ParseoVentDesv.PorEleccion[1].PP=Utilidades.LeerValorRegistro(sRecord, "Valor");
		ParseoVentDesv.PorEleccion[1].Cant=1;
		
		ParseoVentDesv.ModificarHabilidades(sRecord, "VentDes", true);
		ParseoVentDesv.ModificarBaseDeHabilidades(sRecord, true);
	else
		window.parentcontrol.window.Mensajes.setVisible(true);
		window.parentcontrol.window.Mensajes2.setVisible(false);
		window.parentcontrol.window.Mensajes.setFont("FuenteMensajesError");
		window.parentcontrol.window.Mensajes.setValue(Informacion.CadenasInfo["ErrorNoEsVentDesv"]);
	end;
end
