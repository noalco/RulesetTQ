    <LUACode>function onInit()
	setValue(nil, nil);
end

function onDrop(x, y, dragdata)
	local sClass,sRecord = dragdata.getShortcutData();
	if sClass=="VentanaEtniaSidebar" then
		if EtniaPermitida(sRecord) then
			window.parentcontrol.window.Mensajes.setValue("");
			
			local _, sRecordAct=getValue();
			ParseoHabilidades.ResetDatosEtnia();
			
			Religion.ResetearReligiones();
			Religion.ParsearReligiones(Utilidades.LeerValorRegistro(sRecord, "Religion"))
						
			setValue(dragdata.getShortcutData());
			window.NombreEtnia.Actualizar(sRecord);
			
			ParseoHabilidades.IdiomaNativo=Utilidades.LeerValorRegistro(sRecord, "IdiomaNativo");
			
			ParseoHabilidades.ModificarHabilidades(sRecord, "etnia", true);
			local sPathNombre="";
			
			window.MaxTamanioEtnia.setValue(Utilidades.LeerValorRegistro(sRecord, "TamanioMaximo"));

			window.PPEtnia.setValue(Utilidades.LeerValorRegistro(sRecord, "PP"));
			
			ParseoProfesiones.ObtenerProfesionesPermitidas(Utilidades.LeerValorRegistro(sRecord, "Profesiones"));
			local sProfesion=window.parentcontrol.window.SubventanaWizardProfesion.subwindow.NombreProfesion.getValue();
			if sProfesion~="" then
				if not ParseoProfesiones.ProfesionPermitida(sProfesion) then
					ParseoProfesiones.ResetearHabilidadesProfesion();
					window.Mensajes.setVisible(true);
					window.Mensajes2.setVisible(false);
					window.parentcontrol.window.Mensajes.setFont("FuenteMensajesError");
					window.parentcontrol.window.Mensajes.setValue(Informacion.CadenasInfo["ErrorProfesionNoPermitida"]);
				end
			end
			
			ParseoHabilidades.QuitarGratuitos(Utilidades.LeerValorRegistro(sRecord, "HabilidadesSinGratuitos"));
			
			window.HabilidadDeCultura.setVisible(ParseoHabilidades.bHabilidadDeCultura);
			window.ArtesaniaEtnia.setVisible(ParseoHabilidades.bArtesania);
			
			ParseoVentDesv.AsignarVentDesvPorEtnia(window.parentcontrol.window, Utilidades.LeerValorRegistro(sRecord, "VentajasDesventajas"));
			
			window.parentcontrol.window.BotonProfesion.setVisible(true);
			
			window.parentcontrol.window.Mensajes.setValue("");
		else
			window.parentcontrol.window.Mensajes.setVisible(true);
			window.parentcontrol.window.Mensajes2.setVisible(false);
			window.parentcontrol.window.Mensajes.setFont("FuenteMensajesError");
			window.parentcontrol.window.Mensajes.setValue(Informacion.CadenasInfo["ErrorEtniaNoPermitida"]);
		end
	else
		window.parentcontrol.window.Mensajes.setVisible(true);
		window.parentcontrol.window.Mensajes2.setVisible(false);
		window.parentcontrol.window.Mensajes.setFont("FuenteMensajesError");
		window.parentcontrol.window.Mensajes.setValue(Informacion.CadenasInfo["ErrorNoEsEtnia"]);
	end;
end

function EtniaPermitida(sRecord)
	if CharWizard.bQuitarRestricciones then return true; end -- Sin restricciones
	
	local sEspecie=window.SeleccionEspecie.getValue();
	local PermitidoAMereni=Utilidades.LeerValorRegistro(sRecord, "RestringidoAMereni");
	
	local RestringidoAMestizo=Utilidades.LeerValorRegistro(sRecord, "RestringidoAMestizo");
	
	if PermitidoAMereni==0 and RestringidoAMestizo==0 then return true;
	elseif PermitidoAMereni==1 and RestringidoAMestizo==1 and (sEspecie=="Mereni" or sEspecie=="Mestizo") then return true;
	elseif PermitidoAMereni==1 and sEspecie=="Mereni" then return true;
	elseif RestringidoAMestizo==1 and sEspecie=="Mestizo" then return true;
	else return false;
	end
end
