    <LUACode>function onValueChanged()
	if getValue()&lt;=15 then		-- Niño: MaxTamanioEdad 2, Autoridad -2
		window.PPEdad.setValue(12);
		window.LabelTipoEdad.setValue("Niño");
		window.MaxTamanioEdad.setValue(2);
		window.MaxAtractivo.setValue(3);
		ParseoHabilidades.nBonoAutoridadEdad=-2;
	elseif getValue()&lt;=20 then	-- Joven: Autoridad -1
		window.PPEdad.setValue(6);
		window.LabelTipoEdad.setValue("Joven");
		window.MaxTamanioEdad.setValue(4);
		window.MaxAtractivo.setValue(3);
		ParseoHabilidades.nBonoAutoridadEdad=-1;
	elseif getValue()&lt;=35 then	-- Adulto
		window.PPEdad.setValue(0);
		window.LabelTipoEdad.setValue("Adulto");
		window.MaxTamanioEdad.setValue(4);
		window.MaxAtractivo.setValue(3);
		ParseoHabilidades.nBonoAutoridadEdad=0;
	elseif getValue()&lt;=49 then	-- Mayor
		window.PPEdad.setValue(-2);
		window.LabelTipoEdad.setValue("Mayor");
		window.MaxTamanioEdad.setValue(4);
		window.MaxAtractivo.setValue(3);
		ParseoHabilidades.nBonoAutoridadEdad=0;
	elseif getValue()&lt;=64 then	-- Viejo: MaxAtractivo 2, Autoridad +1
		window.PPEdad.setValue(-3);
		window.LabelTipoEdad.setValue("Viejo");
		window.MaxTamanioEdad.setValue(4);
		window.MaxAtractivo.setValue(2);
		ParseoHabilidades.nBonoAutoridadEdad=1;
	else						-- Anciano: MaxAtractivo 2, MaxTamanioEdad 2, Autoridad +2
		window.PPEdad.setValue(-5);
		window.LabelTipoEdad.setValue("Anciano");
		window.MaxTamanioEdad.setValue(2);
		window.MaxAtractivo.setValue(2);
		ParseoHabilidades.nBonoAutoridadEdad=2;
	end
	if ParseoProfesiones.HabilidadesDeProfesion[window.LabelTipoEdad.getValue()] then
		ParseoProfesiones.numHabilidadesProfesion=ParseoProfesiones.HabilidadesDeProfesion[window.LabelTipoEdad.getValue()];
	else
		ParseoProfesiones.numHabilidadesProfesion=60;
	end
	window.PtosDisponiblesConEspecieYEdad.Actualizar();
--	Informacion.MostrarInfo(window.parentcontrol.window, window.LabelTipoEdad.getValue(), "Edad: "..window.LabelTipoEdad.getValue());
end
</LUACode>
