    <LUACode>function onValueChanged()
	local bAlAzar=getValue()=="Al azar";
	window.PtosDisponiblesConEspecieYEdad.Actualizar();
	window.DeCuerpoAMente.setVisible(bAlAzar);
	window.DeMenteACuerpo.setVisible(bAlAzar);
	window.DeMenteAEspiritu.setVisible(bAlAzar);
	window.DeEspirituAMente.setVisible(bAlAzar);
	window.LanzarDadosPtos.setVisible(bAlAzar);
	window.LabelPuntos.setVisible(not bAlAzar);
	window.LabelDisponibles.setVisible(not bAlAzar);
	window.LabelGastados.setVisible(not bAlAzar);
	window.LabelRestantes.setVisible(not bAlAzar);
	window.PtosGastados.setVisible(not bAlAzar);
	window.PtosDisponiblesConEspecieYEdad.setVisible(not bAlAzar);
	window.PtosGastados.setVisible(not bAlAzar);
	window.PtosRestantes.setVisible(not bAlAzar);
	window.BaseCuerpo.setReadOnly(bAlAzar);
	window.BaseMente.setReadOnly(bAlAzar);
	window.BaseEspiritu.setReadOnly(bAlAzar);
	window.MaxMente.Calcular();
	window.MaxEspiritu.Calcular();
	if bAlAzar then
		window.LanzarDadosPtos.onButtonPress();
		ChequeoPJ.PartesCompletadas["Estadísticas"].ErrorRepartoExcesivo=false;
		ChequeoPJ.PartesCompletadas["Estadísticas"].ErrorRepartoIncompleto=false;
	else
		window.PtosDisponibles.setValue(19);
		window.BaseCuerpo.setValue(3);
		window.BaseMente.setValue(3);
		window.BaseEspiritu.setValue(3);
	end
--	Informacion.MostrarInfo(window.parentcontrol.window, getValue(), "Características generadas "..string.lower(getStringValue()));
end

