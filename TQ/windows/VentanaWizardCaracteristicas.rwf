    <LUACode>function onValueChanged()
	window.PtosDisponiblesConEspecieYEdad.Actualizar();
	window.MaxMente.Calcular();
	window.MaxEspiritu.Calcular();

	if getValue()=="Humano" then window.PPEspecie.setValue(0);
	elseif getValue()=="Mestizo" then window.PPEspecie.setValue(-4);
	else window.PPEspecie.setValue(-8);
	end
--	Informacion.MostrarInfo(window.parentcontrol.window, getValue(), "Especie "..getStringValue());
	
	local _, sRecord=window.LinkEtnia.getValue();
	if sRecord and sRecord~="" and not window.LinkEtnia.EtniaPermitida(sRecord) then
		--ParseoHabilidades.ModificarHabilidades(sRecord, "etnia", false);
		ParseoHabilidades.ResetDatosEtnia();
		window.LinkEtnia.setValue(nil, nil);
		window.NombreEtnia.setValue("");
		window.parentcontrol.window.Mensajes.setVisible(true);
		window.parentcontrol.window.Mensajes2.setVisible(false);
		window.parentcontrol.window.Mensajes.setFont("FuenteMensajesError");
		window.parentcontrol.window.Mensajes.setValue(Informacion.CadenasInfo["ErrorCambioEspecie"]);
	end
end

