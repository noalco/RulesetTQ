    <LUACode>function onValueChanged()
	if getValue()~="" then
		ParseoHabilidades.sArtesania=getValue();
		ChequeoPJ.PartesCompletadas["Estadísticas"].ErrorArtesaniaEtnia=getValue()=="Artesanía";
	else
		ParseoHabilidades.sArtesania="";
		ChequeoPJ.PartesCompletadas["Estadísticas"].ErrorArtesaniaEtnia=true;
	end
end

function onVisibilityChanged()
	ChequeoPJ.PartesCompletadas["Estadísticas"].ErrorArtesaniaEtnia=isVisible();
end
